    <!-- http://purl.obolibrary.org/obo/NCRO_0100295 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/NCRO_0100295">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCRO_0100129"/>
