        <obo:IAO_0000111>PGSF1 lncRNA family</obo:IAO_0000111>
        <obo:IAO_0000118>ENSG00000176840;MIR7-3HG;Huh7; PGSF1; C19orf30; LINC00306; NCRNA00306</obo:IAO_0000118>
